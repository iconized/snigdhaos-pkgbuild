source=("$pkgname.tar.gz")
sha512sums=('f911292a457968a1a1c02a5b2c1cabcc9d2c490610a5fb032cfcc0e1507cc41350331645cb711754f42bfa921bb296edd7260e3c8357176ab3453b6bf4c19247')

package() {
  mkdir -p "${pkgdir}/usr/share/pacman/keyrings"

  cp -rf "${srcdir}"/usr/share/pacman/keyrings/* "${pkgdir}/usr/share/pacman/keyrings/"
}
